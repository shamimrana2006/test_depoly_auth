{
  "name": "test_server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "sdev": "nest start --watch",
    "mg": "npx prisma migrate dev && npx prisma generate",
    "reset": "npx prisma migrate reset --force && npx prisma generate",
    "mgsdev": "npx prisma migrate dev && npx prisma generate && nest start --watch",
    "rmgsdev": "rmdir /s /q prisma\\migrations 2>nul & rmdir /s /q prisma\\generated 2>nul & npx prisma migrate reset --force && npx prisma migrate dev && npx prisma generate && nest start --watch",
    "git": "git add . && git commit -m \"update emergency\" && git push",
    "prisma:deploy": "npx prisma generate && npx prisma migrate deploy",
    "server:run:under:dockerimage": "npx prisma generate && npx prisma migrate deploy && node dist/src/main",
    "studio": "npx prisma studio"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.971.0",
